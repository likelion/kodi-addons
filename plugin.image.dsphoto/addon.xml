<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.image.dsphoto"
       name="DS Photo"
       version="1.0.0"
       provider-name="Leonid Mokrushin">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             provides="image"
             library="default.py">
    <provides>image</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Провайдер контента из Synology Photo Station</summary>
    <description>
      Быстрое пролистывание и просмотр фотографий и видео из Synology Photo Station
    </description>
    <platform>all</platform>
  </extension>
</addon>
